<h1>Users#index</h1>
<p>Find me in app/views/user/index.html.erb</p>
<h1 class="ui left floated header">
  <%=current_user.email%>
</h1>
<% @users = User.all %>
<% @users.each do |user| %>
  <% if user != current_user %>
    <%= user.email%>
    <%= form_tag ({controller: 'friendships', action: 'create', method: 'post', id: user.id}) do %>
      <input class="ui basic button" type=submit value='Dodaj do znajomych'>
    <% end %>

  <%end%>
<% end %>
