<% current_user.not_friends.sample(5).each do |user| %>
  <table id="add_to_friend">
    <tr>
      <td>  <%= link_to user.email, user_path(user)%> </td>
      <td> <%= link_to "Dodaj do znajomych", {controller: 'friendships', action: 'create', method: :post, id: user.id, remote: true}, class: "create" %> </td>
    <tr>
  </table>
<% end %>
